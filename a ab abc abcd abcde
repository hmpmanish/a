#include <stdio.h>
#include <conio.h>

void main() {
    int i, j;
    char ch;

    clrscr(); // optional, console clear

    for(i = 1; i <= 5; i++) {       // rows
        ch = 'A';                    // start from 'A' in each row
        for(j = 1; j <= i; j++) {   // columns
            printf("%c", ch);
            ch++;                    // next letter
        }
        printf("\n");
    }

    getch();
}
